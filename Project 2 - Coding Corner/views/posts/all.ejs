<%- include('../partials/header') %>    

<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Topics Covered</th>
        <th>Private?</th>
      </tr>
    </thead>
    <tbody>
      <% posts.forEach(function(p) { %>
        <tr>
          <td><%= p.title %></td>
          
          <td><%= p.topics %></td>
          <td><%= p.private %></td>
          <td><a href="/posts/<%= p._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>

